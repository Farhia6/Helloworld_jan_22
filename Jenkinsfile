pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Build step'
                sleep 10
            } 
        }
        stage('Test') {
            steps {
                echo 'Test step'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy step'
              sleep 10
            }
        }
        stage('Docker') {
            steps {
                echo 'Image step'
            }
        }
    }
}
